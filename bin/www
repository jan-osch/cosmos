#!/usr/bin/env node

/**
 * Module dependencies.
 */

var app = require('../app');
var debug = require('debug')('mosway:server');
var http = require('http');

/**
 * Get port from environment and store in Express.
 */

var port = process.env.PORT || 3000;
console.log('App listening on port:', port);
app.listen(port);
